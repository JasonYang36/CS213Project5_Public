<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.application.cafe">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_title"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.CS213Project5">
        <activity
            android:name=".CurrentOrder"
            android:label="Current Order"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".OrderDonuts"
            android:label="Order Donuts"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".StoreOrdersPage"
            android:label="Store Orders"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".OrderCoffee"
            android:label ="Order Coffee"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>